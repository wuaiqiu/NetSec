敏感文件夹
	
	"Index of" inurl:"/$Recycle.Bin/"					：windows回收站

	inurl:trash intitle:index.of						：linux回收站

	powered by h5ai								：h5ai目录分类程序

	inurl:"wp-content/uploads/private"					：wordpress私人上传文件
	
	inurl:admin inurl:uploads						：admin上传目录
	
	inurl:/fckeditor/editor/plugins/ajaxfilemanager/ajaxfilemanager.php 	: ajax文件管理器
	
	Hostinger © 2016. All rights reserved inurl:default.php  		：Hostinger免费主机默认管理目录页面

	(intext:"index of /.git") ("parent directory")  			：git配置文件

	"sql" "parent" intitle:index.of -injection   				：sql数据库

	
漏洞服务器

	
	"dirLIST - PHP Directory Lister" "Banned files: php | php3 | php4 | php5 | htaccess | htpasswd | asp | aspx" "index of" ext:php	
										：上传漏洞

	allinurl:moadmin.php -google -github					：moadmin远程代码执行


安全日志

	inurl:zabbix.php AND intext:"Zabbix SIA" 				：ZABBIX web型网络监控系统

	intitle:"Nessus Scan Report" ext:html					：nessus扫描报告

	"generated by Munin" inurl:index -intext:index localhost		：munin监控系统

	 inurl:"/weathermap/weathermap-cacti-plugin.php"			：Cacti网络拓扑

	intext:"Web Application Report" intext:"This report was created by IBM Security AppScan" ext:pdf	
										：appscan扫描报告

	"Web Application Assessment Report" ext:pdf				：HP WebInspect Scan Report

	intitle:"bandwidthd" "programmed by david hinkle, commissioned by derbytech wireless networking."	
										：BandwidthD网络流量检测和分析


密码文件

	inurl:_vti_pvt/administrators.pwd					：MS FrontPage 密码文件

	inurl:"sitemanager.xml" ext:xml -git					：FileZilla密码文件

	intext:DB_PASSWORD || intext:"MySQL hostname" ext:txt			：WordPress的mysql数据库配置文件

	"resources.db.params.password" ext:ini -git				：zendframework的数据库配置文件（mvc）

	"mysqli_connect" ext:inc						：mysql密码文件

	" End Stealer " ext:txt							：网络密码窃取器

	"FileZilla" inurl:"recentservers.xml" -git				：FileZilla密码文件

	"Stealer by W33DY" ext:txt						：网络密码窃取器	

	 filetype:bak inurl:php "mysql_connect"					：MySQL密码备份

	"END_FILE" inurl:"/password.log"					：password日志

	inurl:proftpdpasswd							：proftpd密码 

	
	-------------------------------------------xls密码文件-------------------------------------------------------
	
	inurl:wp-content/uploads filetype:xls | filetype:xlsx password		：

	intext:http | https intext:login | logon intext:password | passcode filetype:xls | filetype:xlsx

	inurl:"ftp" intext:"user" | "username" | "userID" | "user ID" | "logon" | "login" intext:"password" | "passcode" filetype:xls | filetype:xlsx

	intext:smtp | pop3 intext:login | logon intext:password | passcode filetype:xls | filetype:xlsx

	-----------------------------------------------------------------------------------------------------------

	inurl:etc -intext:etc ext:passwd					：linux的passwd文件

	intext:DB_PASSWORD ext:env						：全局变量文件

	ext:csv intext:"password"						：密码文件

	inurl:"security/xamppdirpasswd.txt"					：xampp密码文件

	
		
漏洞公告

	inurl:".php?cat=" inurl:"'"							：sql注入

	inurl:"/admin/index.php?msg=" inurl:"%20"					：xss
